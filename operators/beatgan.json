{
	"conf": {
		"version": "1.0",
		"type": "operator",
		"operator": {
			"name": "beatgan",
			"cmd": "beatgan.py",
			"type": "python",
			"version": "1.0",
			"description": "时间序列异常检测",
			"category": "时间序列挖掘",
			"developer": "中科学院网络数据科学与技术重点实验室",
            "contributors":"Bin Zhou, 1009675047@qq.com",
			"input": [
				{
					"name": "input",
					"format": "csv",
					"description": "输入文件名"
				}
			],
			"argument": [
				{
					"name": "model",
					"type": "string",
					"required": "true",
					"default": "/",
					"description": "模型路径"
				},
				{
					"name": "attrlabels",
					"type": "string",
					"required": "true",
					"default": "1,2,3",
					"description": "时间序列的列名，以,隔开"
				},
				{
					"name": "network_config",
					"type": "string",
					"required": "true",
					"default": "beatgan_config.json",
					"description": "网络结构配置文件"
				}
			],
			"output": [
				{
					"name": "output",
					"format": ".csv",
					"description": "输出异常分值"
				}
			],
			"requirements": {
				"type": "string",
				"default": "./requirements.txt"
			}
		}
	}
}
