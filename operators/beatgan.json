{
	"conf": {
		"version": "1.0",
		"type": "operator",
		"operator": {
            "name": "beatgan",
            "cmd": "beatgan.py",
            "type": "python",
            "version": "1.0",
            "description": "时间序列异常检测，contributors: Bin Zhou<1009675047@qq.com>",
            "category": "时间序列挖掘",
            "developer": "中国科学院网络数据科学与技术重点实验室",
            "input": [
                {
                    "name": "input",
                    "format": "pkl",
                    "description": "输入文件名（分割好的时间序列）"
                },
                {
                    "name": "model",
                    "type": "string",
                    "required": "true",
                    "default": "/",
                    "description": "模型路径"
                }
            ],
            "argument": [

            ],
	    "config": [
                {
                    "name": "network_config",
                    "type": "string",
                    "required": "true",
                    "default": "beatgan_config.json",
                    "description": "网络结构配置文件"
                }
	    ],
            "output": [
                {
                    "name": "output",
                    "format": ".csv",
                    "description": "输出异常分值",
                    "table": [
                        {
                            "name": "异常分值",
                            "type": "float",
                            "min": "0",
                            "max": "",
                            "description": "每一段的异常分值"
                        }
                    ]
                },
                {
                    "name": "output1",
                    "format": ".pkl",
                    "required": "false",
                    "description": "原始时间序列段"
                },
                {
                    "name": "output2",
                    "format": ".pkl",
                    "required": "false",
                    "description": "重构时间序列段"
                },
                {
                    "name": "output3",
                    "format": ".csv",
                    "required": "false",
                    "description": "每段序列每一时刻异常分值"
                }
            ],
            "requirements": {
                "type": "string",
                "default": "./requirements.txt"
            }
        }
    }
}
