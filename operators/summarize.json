{
	"conf": {
		"version": "0.1",
		"type": "operator",
		"operator": {
			"name": "summarize",
			"cmd": "summarize.py",
			"type": "python",
			"version": "1.0",
			"description": "基于 MDL 的图摘要",
			"category": "图挖掘",
